    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile_nav_graph"
    app:startDestination="@id/profile_contact_item">

    <fragment
        android:id="@id/profile_contact_item"
        android:name="com.homelab.appointmentadmin.ui.customer.contact.CustomerContactFragment"
        android:label="CustomerContactFragment"
        tools:layout="@layout/fragment_customer_contact">
        <action
            android:id="@+id/action_profile_contact_item_to_customerProfileEditFragment"
            app:destination="@id/customerProfileEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/customerProfileEditFragment"
        android:name="com.homelab.appointmentadmin.ui.customer.edit.CustomerProfileEditFragment"
        android:label="CustomerProfileEditFragment"
        tools:layout="@layout/fragment_customer_profile_edit"/>
